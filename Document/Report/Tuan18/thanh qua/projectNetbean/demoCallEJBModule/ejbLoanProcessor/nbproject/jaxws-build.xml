<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-LoanProcessor" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/LoanProcessor"/>
        <property name="service-wsdl-LoanProcessor" location="${meta.inf}/xml-resources/web-services/LoanProcessor/wsdl/loanProcessorWrapper.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/LoanProcessor" destdir="${build.generated.dir}/jax-wsCache/service/LoanProcessor" wsdl="${service-wsdl-LoanProcessor}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-LoanProcessor}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/LoanProcessor"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/LoanProcessor">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-LoanProcessor" location="${meta.inf}/wsdl/LoanProcessor"/>
        <property name="sourceLocation-LoanProcessor" location="${meta.inf}/xml-resources/web-services/LoanProcessor/wsdl/"/>
        <copy todir="${targetLocation-LoanProcessor}">
            <fileset dir="${sourceLocation-LoanProcessor}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-LoanProcessor" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/LoanProcessor"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/org/netbeans/j2ee/wsdl/ejbloanprocessor/loanprocessor"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-LoanProcessor"/>
</project>
