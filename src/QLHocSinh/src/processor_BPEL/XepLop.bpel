<?xml version="1.0" encoding="UTF-8"?>
<process
    name="XepLop"
    targetNamespace="http://enterprise.netbeans.org/bpel/QLHocSinh/XepLop"
    xmlns:tns="http://enterprise.netbeans.org/bpel/QLHocSinh/XepLop"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/QLHocSinh/XepLop" location="../interface_WSDL/XepLop.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/QLHocSinh/DB_XepLop" location="../DB_WSDL/insert/DB_XepLop.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/DB_XepLop" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/XepLop" partnerLinkType="tns:XepLop" myRole="XepLopPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="ExecuteOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/DB_XepLop" messageType="tns:outputMsg"/>
        <variable name="ExecuteIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/DB_XepLop" messageType="tns:inputMsg"/>
        <variable name="XepLopOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/XepLop" messageType="tns:XepLopOperationResponse"/>
        <variable name="XepLopOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/XepLop" messageType="tns:XepLopOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="XepLopOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/XepLop" portType="tns:XepLopPortType" variable="XepLopOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="XepLopOperationIn" part="idHocSinh"/>
                <to>$ExecuteIn.part/ns0:id_hocsinh_</to>
            </copy>
            <copy>
                <from variable="XepLopOperationIn" part="idLop"/>
                <to>$ExecuteIn.part/ns0:id_lop_</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="PartnerLink2" operation="execute" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/DB_XepLop" portType="tns:jdbcPortType" inputVariable="ExecuteIn" outputVariable="ExecuteOut"/>
        <assign name="Assign2">
            <copy>
                <from>$ExecuteOut.part/ns0:returnValue</from>
                <to variable="XepLopOperationOut" part="result"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="XepLopOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QLHocSinh/XepLop" portType="tns:XepLopPortType" variable="XepLopOperationOut"/>
    </sequence>
</process>

