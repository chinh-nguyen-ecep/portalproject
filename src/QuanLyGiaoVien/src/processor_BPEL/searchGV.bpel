<?xml version="1.0" encoding="UTF-8"?>
<process
    name="searchGV"
    targetNamespace="http://enterprise.netbeans.org/bpel/QuanLyGiaoVien/searchGV"
    xmlns:tns="http://enterprise.netbeans.org/bpel/QuanLyGiaoVien/searchGV"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema" xmlns:ns1="http://xml.netbeans.org/schema/EschoolinkObject" xmlns:ns2="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/newWSDL">
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/searchGV" location="../interface_WSDL/searchGV.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/newWSDL" location="../DB_WSDL/WSDL/searchDSGV.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV" location="../interface_WSDL/tracuuGV.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/newWSDL" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="PartnerLink3" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV" partnerLinkType="tns:tracuuGV" partnerRole="tracuuGVPortTypeRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/searchGV" partnerLinkType="tns:searchGV" myRole="searchGVPortTypeRole"/>
    </partnerLinks>
    <variables>
       
        <variable name="SearchGVOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/searchGV" messageType="tns:searchGVOperationRequest"/>
        <variable name="FindOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/newWSDL" messageType="tns:outputMsg">
            <sxed2:editor>
                <sxed2:predicate path="$FindOut.part/ns0:giaovien_Record[$Variable1]" source="from"/>
            </sxed2:editor>
        </variable>
        <variable name="FindIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/newWSDL" messageType="tns:inputMsg"/>
        <variable name="SearchGVOperationOut1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/searchGV" messageType="tns:searchGVOperationResponse">
            <sxed2:editor>
                <sxed2:predicate path="$SearchGVOperationOut1.part1/ns1:GiaoVien[$Variable1]" source="to"/>
            </sxed2:editor>
        </variable>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="searchGVOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/searchGV" portType="tns:searchGVPortType" variable="SearchGVOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from>concat('%', $SearchGVOperationIn.quequan, '%')</from>
                <to>$FindIn.part/ns0:giaovien_Record/ns0:quequan</to>
            </copy>
            <copy>
                <from>concat('%', $SearchGVOperationIn.cmnd, '%')</from>
                <to>$FindIn.part/ns0:giaovien_Record/ns0:cmnd</to>
            </copy>
            <copy>
                <from>concat('%', $SearchGVOperationIn.chucvu, '%')</from>
                <to>$FindIn.part/ns0:giaovien_Record/ns0:chucvu</to>
            </copy>
            <copy>
                <from>concat('%', $SearchGVOperationIn.tengiaovien, '%')</from>
                <to>$FindIn.part/ns0:giaovien_Record/ns0:tengiaovien</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="PartnerLink2" operation="find" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/newWSDL" portType="tns:jdbcPortType" inputVariable="FindIn" outputVariable="FindOut"/>
        <if name="If1">
            <condition>count($FindOut.part/ns0:giaovien_Record) &gt; 0</condition>
            <scope name="Scope1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV">
                <variables>
                        <variable name="TracuuGVOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV" messageType="tns:tracuuGVOperationResponse"/>
                            <variable name="TracuuGVOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV" messageType="tns:tracuuGVOperationRequest"/>
                            <variable name="Variable1" type="xs:long"/>
                    </variables>
                <sequence name="Sequence2">
                        <assign name="Assign4">
                                <copy>
                                        <from>1</from>
                                            <to variable="Variable1"/>
                                    </copy>
                            </assign>
                            <while name="While1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV">
                                <condition>count($FindOut.part/ns0:giaovien_Record) &gt;= $Variable1</condition>
                                    <sequence name="Sequence1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV">
                                        <assign name="Assign2">
                                                <copy>
                                                        <from>$FindOut.part/ns0:giaovien_Record[$Variable1]/ns0:id_giaovien
                                    <sxed2:editor>
                                                                        <sxed2:predicate path="$FindOut.part/ns0:giaovien_Record[$Variable1]" source="from"/>
                                                                    </sxed2:editor>
                                                            </from>
                                                            <to variable="TracuuGVOperationIn" part="id_gv"/>
                                                    </copy>
                                            </assign>
                                            <invoke name="Invoke2" partnerLink="PartnerLink3" operation="tracuuGVOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/tracuuGV" portType="tns:tracuuGVPortType" inputVariable="TracuuGVOperationIn" outputVariable="TracuuGVOperationOut"/>
                                            <assign name="Assign3">
                                                <copy>
                                                        <from variable="TracuuGVOperationOut" part="result"/>
                                                            <to>$SearchGVOperationOut1.part1/ns1:GiaoVien[$Variable1]
                                        <sxed2:editor>
                                                                        <sxed2:predicate path="$SearchGVOperationOut1.part1/ns1:GiaoVien[$Variable1]" source="to"/>
                                                                    </sxed2:editor>
                                                            </to>
                                                    </copy>
                                                    <copy>
                                                        <from>$Variable1 + 1</from>
                                                            <to variable="Variable1"/>
                                                    </copy>
                                            </assign>
                                    </sequence>
                            </while>
                    <reply name="Reply2" partnerLink="PartnerLink1" operation="searchGVOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyGiaoVien/searchGV" portType="tns:searchGVPortType" variable="SearchGVOperationOut1"/>
                </sequence>
            </scope>
           
        </if>
    </sequence>
</process>











