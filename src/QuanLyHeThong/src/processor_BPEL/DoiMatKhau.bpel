        <partnerLink name="PL_DoiMatKhau" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHeThong/DoiMatKhau" partnerLinkType="tns:DoiMatKhau" myRole="DoiMatKhauPortTypeRole"/>
