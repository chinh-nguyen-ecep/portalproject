                            <to>$ExecuteThemCTTKBIN.part/ns1:id_monhoc</to>
                        </copy>
                        <copy>
                            <from>$ThemTKBOperationIn.thoikhoabieu/ns0:ChiTietThoiKhoaBieu[$ForEach1Counter]/ns0:tietThuMay
                                <sxed2:editor>
                                    <sxed2:predicate path="$ThemTKBOperationIn.thoikhoabieu/ns0:ChiTietThoiKhoaBieu[$ForEach1Counter]" source="from"/>
                                </sxed2:editor>
                            </from>
                            <to>$ExecuteThemCTTKBIN.part/ns1:tietthumay</to>
                        </copy>
                        <copy>
