                            <to>$ExecuteThemCTTKBIN.part/ns1:id_thoikhoabieu</to>
                        </copy>
                    </assign>
                    <invoke name="Invoke2" partnerLink="PartnerLink3" operation="execute" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyThoiKhoaBieu/insert_chitiettkb" portType="tns:jdbcPortType" inputVariable="ExecuteThemCTTKBIN" outputVariable="ExecuteThemCTTKBOut"/>
                </sequence>
            </scope>
        </forEach>
        <assign name="Assign3">
            <copy>
                <from>$ExecuteThemTKBOUT.part/ns1:returnValue</from>
                <to variable="ThemTKBOperationOut" part="id_tkb"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="ThemTKBOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyThoiKhoaBieu/ThemTKB" portType="tns:ThemTKBPortType" variable="ThemTKBOperationOut"/>
