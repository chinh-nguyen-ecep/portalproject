        <variable name="ExecuteThemTKBIN" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyThoiKhoaBieu/insert_tkb" messageType="tns:inputMsg"/>
        <variable name="ThemTKBOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyThoiKhoaBieu/ThemTKB" messageType="tns:ThemTKBOperationResponse"/>
        <variable name="ThemTKBOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyThoiKhoaBieu/ThemTKB" messageType="tns:ThemTKBOperationRequest">
            <sxed2:editor>
                <sxed2:predicate path="$ThemTKBOperationIn.thoikhoabieu/ns0:ChiTietThoiKhoaBieu[$ForEach1Counter]" source="from"/>
            </sxed2:editor>
        </variable>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="ThemTKBOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyThoiKhoaBieu/ThemTKB" portType="tns:ThemTKBPortType" variable="ThemTKBOperationIn"/>
        <assign name="Assign1">
