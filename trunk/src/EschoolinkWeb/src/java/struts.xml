<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "struts-2.0.dtd">
<struts>

    <constant name="struts.enable.DynamicMethodInvocation"
		value="false" />
    <constant name="struts.devMode" value="false" />
    <package name="default" extends="struts-default">
        <action name="login" class="Login">
            <result name="success">/index.jsp</result>
            <result name="error">/index.jsp?module=admin&amp;page=error</result>
        </action>
        <action name="logout" class="Logout">
            <result name="success">/index.jsp</result>
            
        </action>
        <action name="sendmessage" class="student.SendMessage" >
            <result name="success">/index.jsp?module=student&amp;page=sendmessageResult</result>
	</action>
        <action name="viewHistory" class="student.ViewHistoryMessage" >
            <result name="success">/WEB-INF/ajax/viewhistory.jsp</result>
	</action>
        <action name="viewFeedback" class="student.viewMessFeedback" >
            <result name="success">/index.jsp?module=student&amp;page=viewFeedback</result>
	</action>
        <action name="viewMessWaitting" class="manager.ViewMessWaitting" >
            <result name="manager">/WEB-INF/ajax/indexPhongGiaoVu.jsp</result>
            <result name="secretary">/WEB-INF/ajax/indexDoanKhoa.jsp</result>
            <result name="teacher">/WEB-INF/ajax/indexTruongKhoa.jsp</result>
	</action>
        <action name="StationUpdateState" class="manager.StationUpdateState" >
            <result name="success">/index.jsp?module=ajax&amp;page=StationUpdateStateResult</result>
	</action>
        <action name="messageSendingStatus" class="manager.MessageSendingStatus" >
            <result name="success">/WEB-INF/ajax/messageSendingStatus.jsp</result>
            <result name="error">/WEB-INF/ajax/error.jsp</result>
	</action>
    </package>
       
    <include file="/quanlyhocsinh/quanlyhocsinh.xml"></include>
    <include file="/Demo/demo.xml"></include>
    <include file="/quanlydiem/quanlydiem.xml"></include>
    <include file="/quanlyhocsinh/quanlyhocsinh.xml"></include>
    <include file="/quanlydiachi/quanlydiachi.xml"></include>
    <include file="/quanlygiaovien/quanlygiaovien.xml"></include>
    <include file="/quanlyhocba/quanlyhocba.xml"></include>
    <include file="/thongtincanhan/thongtincanhan.xml"></include>
    <include file="/quanlythongtinkhoitao/quanlythongtinkhoitao.xml"></include>
    <include file="/quanlythoikhoabieu/quanlythoikhoabieu.xml"></include>
	

</struts>