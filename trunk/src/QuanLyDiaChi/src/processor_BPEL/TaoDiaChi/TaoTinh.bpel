<?xml version="1.0" encoding="UTF-8"?>
<process
    name="TaoTinh"
    targetNamespace="http://enterprise.netbeans.org/bpel/QuanLyHocSinh/TaoTinh"
    xmlns:tns="http://enterprise.netbeans.org/bpel/QuanLyHocSinh/TaoTinh"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" location="../../interface_WSDL/TaoDiaChi/TaoTinh.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" location="../../DB_WSDL/insert/TaoDiaChi/DB_TaoTinh.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" partnerLinkType="tns:TaoTinh" myRole="TaoTinhPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="ExecuteOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" messageType="tns:outputMsg"/>
        <variable name="ExecuteIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" messageType="tns:inputMsg"/>
        <variable name="TaoTinhOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" messageType="tns:TaoTinhOperationResponse"/>
        <variable name="TaoTinhOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" messageType="tns:TaoTinhOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="TaoTinhOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" portType="tns:TaoTinhPortType" variable="TaoTinhOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="TaoTinhOperationIn" part="tenTinh"/>
                <to>$ExecuteIn.part/ns0:tentinh</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="PartnerLink2" operation="execute" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" portType="tns:jdbcPortType" inputVariable="ExecuteIn" outputVariable="ExecuteOut"/>
        <assign name="Assign2">
            <copy>
                <from>$ExecuteOut.part/ns0:returnValue</from>
                <to variable="TaoTinhOperationOut" part="id"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="TaoTinhOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoTinh" portType="tns:TaoTinhPortType" variable="TaoTinhOperationOut"/>
    </sequence>
</process>


