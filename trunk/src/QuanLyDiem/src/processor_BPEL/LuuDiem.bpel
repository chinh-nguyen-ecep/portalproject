<?xml version="1.0" encoding="UTF-8"?>
<process
    name="LuuDiem"
    targetNamespace="http://enterprise.netbeans.org/bpel/QuanLyDiem/LuuDiem"
    xmlns:tns="http://enterprise.netbeans.org/bpel/QuanLyDiem/LuuDiem"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyDiem/LuuDiem" location="../interface_WSDL/LuuDiem.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyDiem/DB_LuuDiem" location="../DB_WSDL/DB_LuuDiem.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PL_DB_LuuDiem" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/DB_LuuDiem" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="PL_LuuDiem" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/LuuDiem" partnerLinkType="tns:LuuDiem" myRole="LuuDiemPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="LuuDiemOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/DB_LuuDiem" messageType="tns:outputMsg"/>
        <variable name="LuuDiemIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/DB_LuuDiem" messageType="tns:inputMsg"/>
        <variable name="LuuDiemOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/LuuDiem" messageType="tns:LuuDiemOperationResponse"/>
        <variable name="LuuDiemOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/LuuDiem" messageType="tns:LuuDiemOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PL_LuuDiem" operation="LuuDiemOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/LuuDiem" portType="tns:LuuDiemPortType" variable="LuuDiemOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="LuuDiemOperationIn" part="id_diem"/>
                <to>$LuuDiemIn.part/ns0:id_diemin</to>
            </copy>
            <copy>
                <from variable="LuuDiemOperationIn" part="diem"/>
                <to>$LuuDiemIn.part/ns0:diemin</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="PL_DB_LuuDiem" operation="execute" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/DB_LuuDiem" portType="tns:jdbcPortType" inputVariable="LuuDiemIn" outputVariable="LuuDiemOut"/>
        <assign name="Assign2">
            <copy>
                <from>$LuuDiemOut.part/ns0:returnValue</from>
                <to variable="LuuDiemOperationOut" part="luudiemBoolean"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PL_LuuDiem" operation="LuuDiemOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyDiem/LuuDiem" portType="tns:LuuDiemPortType" variable="LuuDiemOperationOut"/>
    </sequence>
