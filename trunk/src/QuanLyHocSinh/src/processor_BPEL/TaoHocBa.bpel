            <copy>
                <from>$ExecuteOut.part/ns0:returnValue</from>
                <to variable="TaoHocBaOperationOut" part="id"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink2" operation="TaoHocBaOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoHocBa" portType="tns:TaoHocBaPortType" variable="TaoHocBaOperationOut"/>
