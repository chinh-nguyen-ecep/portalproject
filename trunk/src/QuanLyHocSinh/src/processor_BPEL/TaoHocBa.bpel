    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/DB_hocba_insert" location="../DB_WSDL/insert/DB_hocba_insert.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoHocBa" location="../interface_WSDL/TaoHocBa.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoHocBa" partnerLinkType="tns:TaoHocBa" myRole="TaoHocBaPortTypeRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/DB_hocba_insert" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="ExecuteOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/DB_hocba_insert" messageType="tns:outputMsg"/>
        <variable name="ExecuteIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/DB_hocba_insert" messageType="tns:inputMsg"/>
        <variable name="TaoHocBaOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoHocBa" messageType="tns:TaoHocBaOperationResponse"/>
        <variable name="TaoHocBaOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoHocBa" messageType="tns:TaoHocBaOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink2" operation="TaoHocBaOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/QuanLyHocSinh/TaoHocBa" portType="tns:TaoHocBaPortType" variable="TaoHocBaOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="TaoHocBaOperationIn" part="id_namhoc"/>
                <to>$ExecuteIn.part/ns0:id_namhoc</to>
            </copy>
