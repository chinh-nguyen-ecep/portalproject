<?xml version="1.0" encoding="UTF-8"?>
<process
    name="SecretaryReview"
    targetNamespace="http://enterprise.netbeans.org/bpel/SendMessToDean/SecretaryReview"
    xmlns:tns="http://enterprise.netbeans.org/bpel/SendMessToDean/SecretaryReview"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/SendMessToDean/SecretaryReview" location="WSDLInterface/SecretaryReview.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertMessToSceretary" location="database/DB_insertMessToSceretary.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertHistory" location="database/DB_insertHistory.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_loadStateOfMessToSecretary" location="database/DB_loadStateOfMessToSecretary.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/SendMessToDean/ChangeStateOfHistory" location="internalProcess/ChangeStateOfHistory.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="history" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertHistory" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="secretary" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertMessToSceretary" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="StateOfSecre" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_loadStateOfMessToSecretary" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="setStateHistory" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/ChangeStateOfHistory" partnerLinkType="tns:ChangeStateOfHistory" partnerRole="ChangeStateOfHistoryPortTypeRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/SecretaryReview" partnerLinkType="tns:SecretaryReview" myRole="SecretaryReviewPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="note" type="xs:string"/>
        <variable name="ChangeStateOfHistoryOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/ChangeStateOfHistory" messageType="tns:ChangeStateOfHistoryOperationResponse"/>
        <variable name="ChangeStateOfHistoryOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/ChangeStateOfHistory" messageType="tns:ChangeStateOfHistoryOperationRequest"/>
        <variable name="loadStateOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_loadStateOfMessToSecretary" messageType="tns:outputMsg">
            <sxed2:editor>
                <sxed2:predicate path="$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]" source="from"/>
            </sxed2:editor>
        </variable>
        <variable name="loadStateIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_loadStateOfMessToSecretary" messageType="tns:inputMsg"/>
        <variable name="state" type="xs:string"/>
        <variable name="ExecuteSecretOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertMessToSceretary" messageType="tns:outputMsg"/>
        <variable name="ExecuteSecretIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertMessToSceretary" messageType="tns:inputMsg"/>
        <variable name="ExecuteHistoryOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertHistory" messageType="tns:outputMsg"/>
        <variable name="ExecuteHistoryIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertHistory" messageType="tns:inputMsg"/>
        <variable name="SecretaryReviewOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/SecretaryReview" messageType="tns:SecretaryReviewOperationResponse"/>
        <variable name="SecretaryReviewOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/SecretaryReview" messageType="tns:SecretaryReviewOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="SecretaryReviewOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/SecretaryReview" portType="tns:SecretaryReviewPortType" variable="SecretaryReviewOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="SecretaryReviewOperationIn" part="idMess"/>
                <to>$ExecuteHistoryIn.part/ns0:idMessage</to>
            </copy>
            <copy>
                <from>'Secretary'</from>
                <to>$ExecuteHistoryIn.part/ns0:place</to>
            </copy>
            <copy>
                <from>'wait'</from>
                <to variable="state"/>
            </copy>
        </assign>
        <invoke name="TaoHistory" partnerLink="history" operation="execute" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertHistory" portType="tns:jdbcPortType" inputVariable="ExecuteHistoryIn" outputVariable="ExecuteHistoryOut"/>
        <assign name="Assign2">
            <copy>
                <from>$ExecuteHistoryOut.part/ns0:returnValue</from>
                <to>$ExecuteSecretIn.part/ns0:id_history</to>
            </copy>
            <copy>
                <from variable="SecretaryReviewOperationIn" part="idMess"/>
                <to>$ExecuteSecretIn.part/ns0:idMessage</to>
            </copy>
        </assign>
        <invoke name="TaoMessToSecre" partnerLink="secretary" operation="execute" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_insertMessToSceretary" portType="tns:jdbcPortType" inputVariable="ExecuteSecretIn" outputVariable="ExecuteSecretOut"/>
        <while name="checkState">
            <condition>$state = 'wait'</condition>
            <sequence name="Sequence1">
                <wait name="Wait1">
                    <for>'P0Y0M0DT0H0M30.0S'</for>
                </wait>
                <assign name="Assign3">
                    <copy>
                        <from>$ExecuteSecretOut.part/ns0:returnValue</from>
                        <to>$loadStateIn.part/ns0:id</to>
                    </copy>
                </assign>
                <invoke name="kiemTraState" partnerLink="StateOfSecre" operation="newuntitled_Operation" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/DB_loadStateOfMessToSecretary" portType="tns:jdbcPortType" inputVariable="loadStateIn" outputVariable="loadStateOut"/>
                <assign name="Assign4">
                    <copy>
                        <from>$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]/ns0:state
                            <sxed2:editor>
                                <sxed2:predicate path="$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]" source="from"/>
                            </sxed2:editor>
                        </from>
                        <to variable="state"/>
                    </copy>
                </assign>
            </sequence>
        </while>
        <assign name="Assign6">
            <copy>
                <from>$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]/ns0:state
                    <sxed2:editor>
                        <sxed2:predicate path="$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]" source="from"/>
                    </sxed2:editor>
                </from>
                <to variable="ChangeStateOfHistoryOperationIn" part="state"/>
            </copy>
            <copy>
                <from>$ExecuteHistoryOut.part/ns0:returnValue</from>
                <to variable="ChangeStateOfHistoryOperationIn" part="idHistory"/>
            </copy>
            <copy>
                <from>'x'</from>
                <to variable="ChangeStateOfHistoryOperationIn" part="note"/>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="setStateHistory" operation="ChangeStateOfHistoryOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/ChangeStateOfHistory" portType="tns:ChangeStateOfHistoryPortType" inputVariable="ChangeStateOfHistoryOperationIn" outputVariable="ChangeStateOfHistoryOperationOut"/>
        <assign name="Assign5">
            <copy>
                <from>$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]/ns0:state
                    <sxed2:editor>
                        <sxed2:predicate path="$loadStateOut.part/ns0:DB_loadStateOfMessToSecretary_Record[1]" source="from"/>
                    </sxed2:editor>
                </from>
                <to variable="SecretaryReviewOperationOut" part="result"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="SecretaryReviewOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/SendMessToDean/SecretaryReview" portType="tns:SecretaryReviewPortType" variable="SecretaryReviewOperationOut"/>
    </sequence>
</process>








